apiVersion: k8s.streamnative.io/v1alpha1
kind: SNConsole
metadata:
  # no need to add component to name here as operator will add
  name: "ra-sn-platform"
  namespace: nsa
  labels:
    app: sn-platform
    chart: sn-platform-1.8.0
    release: ra
    heritage: Helm
    cluster: ra-sn-platform
    component: streamnative-console
spec:
  pod:
    metadata:
      labels:
        app: sn-platform
        release: ra
        cluster: ra-sn-platform
        component: streamnative-console
      annotations:
    spec:
      terminationGracePeriodSeconds: 0
      initContainers:
      # This init container will wait for broker to be ready before
      # deploying the pulsar manager
      - name: wait-broker-ready
        image: "streamnative/sn-platform:2.10.2.4"
        imagePullPolicy: IfNotPresent
        command: ["sh", "-c"]
        args:
          - >
            set -e; while [ "$(curl -s -o /dev/null -w '%{http_code}' http://ra-sn-platform-broker:8080/status.html)" -ne "200" ]; do echo "pulsar cluster isn't initialized yet..."; sleep 5; done;
            echo "broker cluster is ready";
        resources:
          limits: {}
          requests:
            cpu: 0.1
            memory: 250Mi
      containers:
        - name: "ra-sn-platform-streamnative-console"
          image: "streamnative/sn-platform-console:v1.14.3"
          imagePullPolicy: IfNotPresent
          resources:
            limits: {}
            requests:
              cpu: 0.1
              memory: 250Mi
          env:
          - name: SPRING_CONFIGURATION_FILE
            value: /pulsar-manager/pulsar-manager/application.properties
          - name: DEFAULT_ORGANIZATION
            value: streamnative
          - name: DEFAULT_NAME
            value: streamnative
          - name: INSTANCE_NAME
            value: pulsar
          - name: CONNECTOR_ENABLED
            value: "true"
          - name: WEB_SERVICE_URL
            value: http://ra-sn-platform-broker.nsa.svc.cluster.local:8080
          - name: KOP_SERVICE_URL
            value: :9093
          - name: BACKEND_DEFAULT_SUPER_USER_ROLE
            value: admin
          volumeMounts:
          - name: streamnative-console-data
            mountPath: /data
          envFrom:
          - secretRef:
              name: ra-sn-platform-vault-secret-env-injection
      volumes:
      - name: streamnative-console-data
        persistentVolumeClaim:
          claimName: "ra-sn-platform-streamnative-console-data"
      serviceAccountName: ra-sn-platform-streamnative-console-acct
