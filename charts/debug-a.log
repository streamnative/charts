apiVersion: "vault.banzaicloud.com/v1alpha1"
kind: "Vault"
metadata:
  name: "ra-sn-platform-vault"
  namespace: nsa
  labels:
    app: sn-platform
    chart: sn-platform-1.8.0
    release: ra
    heritage: Helm
    cluster: ra-sn-platform
    component: vault
spec:
  size: 3
  image: "vault:1.10.4"
  bankVaultsImage: "ghcr.io/banzaicloud/bank-vaults:1.16.0"
  serviceAccount: ra-sn-platform-vault-acct
  serviceType: ClusterIP
  statsdDisabled: true
  vaultLabels:
    component: "vault"
    app: sn-platform
    release: ra
    cluster: ra-sn-platform
  vaultAnnotations:    
    prometheus.io/port: "9091"
    
    
  volumeClaimTemplates:
  - metadata:
      name: "ra-sn-platform-vault-raft"
    spec:
      accessModes: [ "ReadWriteOnce" ]
      resources:
        requests:
          storage: 100Mi
      
  volumeMounts:
    - name: "ra-sn-platform-vault-raft"
      mountPath: /vault/file
  unsealConfig:
    kubernetes:
      secretNamespace: nsa
  istioEnabled: false
  # A YAML representation of a final vault config file.
  # See https://www.vaultproject.io/docs/configuration/ for more information.
  config:
  
    api_addr: http://${.Env.POD_NAME}:8200
    bankVaults:
      probe:
        livenessProbe: {}
        readinessProbe: {}
        startupProbe: {}
    cluster_addr: http://${.Env.POD_NAME}:8201
    listener:
      tcp:
        address: 0.0.0.0:8200
        telemetry:
          unauthenticated_metrics_access: true
        tls_disable: true
    storage:
      raft:
        path: /vault/file
    telemetry:
      statsd_address: ""
    ui: true
  resources:
    vault:
      limits:
        cpu: 200m
        memory: 512Mi
      requests:
        cpu: 100m
        memory: 256Mi
